{
  "expo": {
    "name": "Nitto Ponno",
    "slug": "nittoponno",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/splash.png",
    "backgroundColor": "#facc14",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#facc14"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.shuvajitmaitra.nittoponno",
      "appName": "Nitto Ponno"
    },
    "web": {
      "favicon": "./src/assets/images/icon.jpg",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#facc14",
          "image": "./src/assets/images/splash.png",
          "dark": {
            "image": "./src/assets/images/splash.png",
            "backgroundColor": "#facc14"
          },
          "imageWidth": 200
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "0d8e7f28-3561-4084-b9cb-97e90ae84346"
      }
    }
  }
}
